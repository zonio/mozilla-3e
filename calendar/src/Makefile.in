DEPTH = ../../../../..
topsrcdir = @top_srcdir@
srcdir = @srcdir@
VPATH = @srcdir@

include $(DEPTH)/config/autoconf.mk

MODULE = calendar3e
XPI_NAME = calendar3e

EXTRA_COMPONENTS = \
	calEeeModule.js \
	$(NULL)

EXTRA_SCRIPTS = \
	calEeeCalendar.js \
	calEeeClient.js \
	calEeeMethodQueue.js \
	calEeeProtocol.js \
	calEeeSynchronizer.js \
	$(NULL)

export:: $(EXTRA_SCRIPTS)
	if test ! -d $(FINAL_TARGET)/js; then $(NSINSTALL) -D $(FINAL_TARGET)/js; fi
	$(INSTALL) $^ $(FINAL_TARGET)/js

include $(topsrcdir)/config/rules.mk
