#include "calICalendar.idl"

interface calEeeIClient;
interface nsIMsgIdentity;

/**
 * Behavior specific to EEE calendars.
 */
[scriptable, uuid(2378d543-22ce-4449-9c11-5d3ff736c72e)]
interface calEeeICalendar: calICalendar {

    /**
     * Calendar identifier used in EEE protocol unique in the scope of
     * owner's domain.
     */
    readonly attribute string calspec;

    /**
     * Calendar identifier used in EEE protocol unique in the scope of
     * its owner.
     */
    readonly attribute string calname;

    /**
     * Identity which operates this calendar.
     */
    readonly attribute nsIMsgIdentity identity;

};
