<overlay id="calendar-new-event-attach"
         xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul">

  <stringbundleset id="stringbundleset">
    <stringbundle id="calendar3e-strings"
                  src="chrome://calendar3e/locale/cal3eCalendar.properties"/>
  </stringbundleset>

  <script type="application/x-javascript"
          src="chrome://calendar3e/content/calendarAttachments.js"/>

  <commandset id="itemCommands">
    <command id="cal3e_cmd_attach_file" disable-on-read-only="true"
             oncommand="attachFile()" />
    <command id="cal3e_cmd_open_attachment" disable-on-read-only="true"
             oncommand="cal3e_openAttachment()" />
    <command id="cal3e_cmd_save_as" disable-on-read-only="true"
             oncommand="alert('cal3e_cmd_save_as')" />
  </commandset>

</overlay>
