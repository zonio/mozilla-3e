<?xml-stylesheet type="text/css"
                 href="chrome://messenger/skin/accountManage.css"?>
<!DOCTYPE page [
  <!ENTITY % calendar3e SYSTEM "chrome://calendar3e/locale/calendar3e.dtd">
  %calendar3e;
]>

<page xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
      title="&calendar3e.enable.title;"
      onload="amEnable3e.onLoad()">

  <script type="application/javascript"
          src="chrome://messenger/content/AccountManager.js"/>
  <script type="application/javascript"
          src="chrome://calendar3e/content/am-enable3e.js"/>

  <dialogheader title="&calendar3e.enable.title;"/>

  <description>&calendar3e.enable.description;</description>
  <checkbox id="cal3e-enable-checkbox"
            label="&calendar3e.enable.checkbox;"
            checked="true"/>

</page>
